<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adv"
    android:installLocation="preferExternal"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
 
    <application
        android:icon="@drawable/appimg"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".AdvcontactActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="save"></activity>
        <activity android:name="Search"></activity>
        <activity android:name="Display"></activity>
        <activity android:name="DisplayAllContacts"></activity>
        
    </application>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

</manifest>